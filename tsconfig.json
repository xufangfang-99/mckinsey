{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"],
      "#imports": ["./.nuxt/imports"]
    },
    "skipLibCheck": true,
    "noEmit": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "types": ["@nuxtjs/i18n", "@element-plus/nuxt", "nuxt", "@nuxt/types", "@types/uuid"]
  },
  "include": [
    "./**/*.ts",
    "./**/*.tsx",
    "./**/*.vue",
    "./**/*.json",
    "./components/**/*.vue",
    ".nuxt/nuxt.d.ts",
    "*.d.ts",
    "types/**/*.d.ts"
  ],
  "exclude": ["node_modules", "dist", ".output", ".nuxt"]
}
