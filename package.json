{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nuxt dev --dotenv .env.dev",
    "prod": "nuxt dev --dotenv .env.dev",
    "build:dev": "nuxt build --dotenv .env.dev",
    "build:prod": "nuxt build --dotenv .env.prod",
    "build": "nuxt build --dotenv .env.prod",
    "generate": "nuxt generate --dotenv .env.dev",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint \"{**/*,*}.{js,ts,vue}\"",
    "lint:fix": "prettier --write . && eslint \"{**/*,*}.{js,ts,vue}\" --fix",
    "format": "prettier --write \"**/*.{js,ts,vue,json,css,scss,md}\"",
    "format:all": "prettier --write \"**/*.{js,ts,vue,json,css,scss,md}\" && eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "@nuxt/types": "^2.18.1",
    "@pinia/nuxt": "^0.11.2",
    "dayjs": "^1.11.13",
    "lodash-unified": "^1.0.3",
    "nuxt": "^3.15.4",
    "nuxt-svgo": "^4.0.14",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.2.0",
    "uuid": "^11.0.5",
    "vue": "3.5.13",
    "vue-router": "4.5.0"
  },
  "devDependencies": {
    "@element-plus/nuxt": "^1.1.1",
    "@iconify-json/carbon": "^1.2.6",
